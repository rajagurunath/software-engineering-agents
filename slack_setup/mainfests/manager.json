{
    "display_information": {
        "name": "io-eng-manager"
    },
    "features": {
        "app_home": {
            "home_tab_enabled": false,
            "messages_tab_enabled": true,
            "messages_tab_read_only_enabled": false
        },
        "bot_user": {
            "display_name": "io-eng-manager",
            "always_online": false
        },
        "assistant_view": {
            "assistant_description": "Hi, I am an assistant for project and eng team management.",  
            "suggested_prompts": []
        }
    },
    "oauth_config": {   
        "scopes": {
            "bot": [
                "assistant:write",
                "channels:join",
                "im:history",
                "channels:history",
                "groups:history",
                "chat:write",
                "files:write"
            ]
        }
    },
    "settings": {
        "event_subscriptions": {
            "bot_events": [
                "assistant_thread_context_changed",
                "assistant_thread_started",
                "message.im"
            ]
        },
        "interactivity": {
            "is_enabled": true
        },
        "org_deploy_enabled": false,
        "socket_mode_enabled": true,
        "token_rotation_enabled": false
    }
}